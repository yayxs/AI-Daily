{
  "manifest_version": 3,
  "name": "AI聊天官方入口汇集",
  "version": "0.0.1",
  "description": "将各大AI模型厂商的聊天界面汇集到一个插件中",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.deepseek.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "options_page": "blank.html"
} 